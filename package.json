{
  "name": "yodl-store-webhook",
  "version": "0.0.1",
  "main": "public/server.js",
  "type": "module",
  "scripts": {
    "start": "node public/server.js",
    "dev": "nodemon src/server.ts -e ts --exec tsx",
    "build": "tsc && tsc-alias",
    "watch": "tsc -w",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "generate:client": "tsx src/scripts/IntegrationGenerator.ts"
  },
  "keywords": [
    "yodl",
    "webhooks"
  ],
  "author": "MihkelJ",
  "license": "MIT",
  "description": "Yodl Store Webhook",
  "dependencies": {
    "@selfxyz/core": "^1.0.6",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.10",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-zod-api": "^22.2.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "redis": "^4.7.0",
    "typescript": "^5.7.3",
    "viem": "^2.22.15",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2"
  }
}
